{
	"_version": "1.58.0",
	"sap.app": {
		"id": "ui5.employeecalendar",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			],
			"fallbackLocale": ""
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"invoiceRemote": {
				"uri": "V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.108.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ui5.employeecalendar.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			},
			"invoice": {
				"dataSource": "invoiceRemote"
			}
		},
		"rootView": {
			"viewName": "ui5.employeecalendar.view.App",
			"type": "XML",
			"id": "app",
			"async": true
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "ui5.employeecalendar.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true,
				"clearControlAggregation": false
			},
			"routes": [
				{
					"pattern": "",
					"name": "login",
					"target": "login"
				},
				{
					"pattern": "launchpad",
					"name": "launchpad",
					"target": "launchpadV2"
				},
				{
					"pattern": "main",
					"name": "main",
					"target": "main"
				},
				{
					"pattern": "component/{componentId}",
					"name": "componentView",
					"target": "componentView"
				},
				{
					"pattern": "employees",
					"name": "employeeManagement",
					"target": "employeeManagement"
				},
				{
					"pattern": "expenses",
					"name": "expenseReports",
					"target": "expenseReports"
				},
				{
					"pattern": "tiles/manage",
					"name": "tileManagement",
					"target": "tileManagement"
				},
				{
					"pattern": "settings",
					"name": "settings",
					"target": "settings"
				},
				{
					"pattern": "profile",
					"name": "profile",
					"target": "profile"
				}
			],
			"targets": {
				"login": {
					"viewName": "Login",
					"viewLevel": 0,
					"viewId": "loginView"
				},
				"launchpad": {
					"viewName": "Launchpad",
					"viewLevel": 1,
					"viewId": "launchpadView"
				},
				"launchpadV2": {
					"viewName": "LaunchpadV2",
					"viewLevel": 1,
					"viewId": "launchpadV2View"
				},
				"main": {
					"viewName": "Main",
					"viewLevel": 2,
					"viewId": "mainView"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	}
}
